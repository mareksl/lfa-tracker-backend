{
  "name": "lfa-tracker-backend",
  "version": "1.0.0",
  "main": "app.js",
  "repository": "https://gitlab.com/mareksl/lfa-tracker-backend",
  "author": "mareksl <marekslad@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "mongodb": "^3.1.6",
    "mongoose": "^5.2.17",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0"
  },
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "test": "jest --bail",
    "test-watch": "nodemon --watch src/ --watch tests/ -e js --exec \"yarn test\"",
    "clean": "rimraf ./build && mkdir build",
    "build-server": "babel -d ./build ./src -s",
    "build": "yarn run clean && yarn run build-server",
    "start": "node ./build"
  }
}
