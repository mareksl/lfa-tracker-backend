  "license": "MIT",
  "dependencies": {
    "@meanie/mongoose-upsert-many": "^1.5.1",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "mongodb": "^3.1.6",
    "mongoose": "^5.3.1",
    "mongoose-paginate": "^5.0.3",
    "mongoose-unique-validator": "^2.0.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.0",
    "validator": "^10.8.0",
    "xlsx": "^0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0"
  },
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "test": "jest --bail --verbose --runInBand",
    "test-watch": "nodemon --watch src/ --watch tests/ -e js --exec \"yarn test\"",
    "coverage": "jest --collectCoverageFrom=src/**.js --collectCoverageFrom=!src/index.js --coverage",
    "clean": "rimraf ./build && mkdir build",
    "build-server": "babel -d ./build ./src -s",
    "build": "yarn run clean && yarn run build-server",
    "start": "node ./build"
  }
}
